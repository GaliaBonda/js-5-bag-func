{
    "compilerOptions": {
    
        "target": "es2019",
        "module": "commonjs",
        "lib": [
            "es2020"
        ],
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "outDir": "build",
        "rootDir": "src",
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}